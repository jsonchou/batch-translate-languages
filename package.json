{
  "name": "batch-translate-languages",
  "version": "1.0.0",
  "description": "a Chrome Extension** of accurate download image",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.prod.js",
    "dll:react": "cross-env spa=react webpack -p --progress --config build/webpack.dll.js",
    "dll:vue": "cross-env spa=vue webpack -p --progress --config build/webpack.dll.js",
    "clean": "cross-env rimraf dist/scripts",
    "stats": "cross-env NODE_ENV=production ANALYZER=stats webpack --profile --json > build/stats.json --config build/webpack.prod.js"
  },
  "repository": "https://github.com/jsonchou/batch-translate-languages",
  "keywords": [
    "batch-translate-languages",
    "download image"
  ],
  "author": {
    "name": "jsonchou",
    "email": "jsonchou@outlook.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.3.0",
    "browser-sync": "^2.26.3",
    "cross-env": "^5.2.0",
    "css-loader": "^6.7.3",
    "cssnano": "^4.1.8",
    "eslint": "^5.11.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^6.5.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^1.2.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^5.75.0",
    "webpack-build-notifier": "^0.1.30",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
